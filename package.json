  "scripts": {
    "start": "node ./scripts/runScript.js ./scripts/start",
    "android": "cross-env ENVFILE=.env.development node ./scripts/runScript.js ./scripts/android",
    "ios": "cross-env ENVFILE=.env.development node ./scripts/runScript.js ./scripts/ios",
    "lint:code": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:code:fix": "npm run lint:code -- --fix",
    "type-check": "tsc",
    "load-env": "node scripts/runScript.js ./scripts/loadEnv",
    "graphql-codegen": "node ./scripts/runScript.js ./scripts/graphqlCodegen",
    "clean:ios": "cd ios && pod deintegrate && pod cache clean -all && pod install",
    "build:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "new_secret_key": "node -e \"console.log(require('crypto').randomBytes(128).toString('base64'));\"",
    "prepare": "husky install",
    "postinstall": "if [ -f './node_modules/.bin/patch-package' ]; then ./node_modules/.bin/patch-package --patch-dir ./config/vendors; fi",
    "precommit": "cross-env ENVFILE=.env.development node ./scripts/runScript ./scripts/precommit"
